{
  "name": "@xprofiler/wsserver",
  "version": "1.0.3",
  "description": "server for xtransit",
  "main": "index.js",
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-xtransit-server --port=9090 --workers=4",
    "stop": "egg-scripts stop --title=egg-server-xtransit-server",
    "dev": "egg-bin dev --port=9090",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "tsc": "rm -rf dist && tsc -p tsconfig.json",
    "autod": "autod"
  },
  "files": [
    "app",
    "config/config.default.js",
    "app.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyj1991/xtransit-server.git"
  },
  "keywords": [
    "xprofiler",
    "easy-monitor",
    "xtransit"
  ],
  "author": "yeekwanvong@gmail.com",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/hyj1991/xtransit-server/issues"
  },
  "homepage": "https://github.com/hyj1991/xtransit-server#readme",
  "devDependencies": {
    "@artus/eslint-config-artus": "^0.0.1",
    "@types/node": "^18.14.1",
    "autod": "^3.1.0",
    "egg-bin": "^4.19.0",
    "eslint": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@xprofiler/injection": "^0.0.5",
    "address": "^1.1.2",
    "egg": "^2.35.0",
    "egg-scripts": "^2.16.0",
    "uuid": "^8.0.0",
    "ws": "^7.3.0"
  }
}